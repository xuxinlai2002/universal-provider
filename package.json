{
  "name": "@uptsmart/universal-provider",
  "version": "1.3.1",
  "description": "OKX Connect Universal Provider",
  "main": "./src/index.js",
  "keywords": [
    "OKXConnect",
    "OKX Connect"
  ],
  "scripts": {
    "build": "npx tsc; yarn buildjs",
    "use:local": " yarn add @okxconnect/core@file:../core/src && npx tsc",
    "buildjs": "tsc && webpack --config webpack.config.cjs --mode production; yarn babel; yarn minify",
    "babel": "babel ./dist/okxconnect_universal.js -o ./dist/okxconnect_universal.min.js",
    "minify": "uglifyjs ./dist/okxconnect_universal.min.js --source-map -cm -o ./dist/okxconnect_universal.min.js"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "OKX Wallet",
  "license": "MIT",
  "dependencies": {
    "@okxconnect/core": "1.3.0",
    "@types/lodash": "^4.17.7",
    "cross-fetch": "^3.1.4",
    "eventemitter3": "^5.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.24.8",
    "@babel/core": "^7.24.9",
    "@babel/preset-env": "^7.24.8",
    "@types/crypto-js": "^4.2.2",
    "@types/lodash": "^4.17.7",
    "assert": "^2.1.0",
    "eslint": "^9.7.0",
    "jest": "^29.7.0",
    "ts-loader": "^9.5.1",
    "typescript": "^5.5.4"
  }
}
